<h3>新規投稿ページ</h3>



<%= form_for @posts do |f| %>
  <div class="field">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>
  <%= f.label :content,"内容" %>
  <%= f.text_field :kansou, :size => 50%>
  <%= f.select :category, [["ASMR系の紙", "ASMR系の紙"], ["フィルム系", "フィルム系"], ["弁当系", "弁当系"]], include_blank: "どの投稿？" %>
  

  <%= f.submit "投稿する" %>
 <% end %>

 


 
<%= link_to "一覧に戻る", posts_path %>
